		"prompts": "2.4.2",
		"undici": "5.22.1"
	},
	"devDependencies": {
		"@favware/npm-deprecate": "1.0.7",
		"@types/prompts": "2.4.4",
		"esbuild-plugin-version-injector": "1.1.0",
